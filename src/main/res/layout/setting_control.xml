<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffee"
    android:focusable="true"
    android:focusableInTouchMode="true">


    <grean.custom.component.ExtendedHorizontalScrollView
        android:id="@+id/control_scroll"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:animateLayoutChanges="true"
        android:scrollbars="none"
        android:fadingEdgeLength="30dp"
        android:requiresFadingEdge="horizontal"
        android:background="@color/gray_light"
        >
        <LinearLayout
            android:id="@+id/scroll_content"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:background="#ffffee">

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                >

                <!--here the whole lines and titiles begin-->
                <TextView
                    android:id="@+id/text_eight_valve"
                    android:layout_width="300dp"
                    android:layout_height="80dp"
                    android:gravity="center"
                    android:text="@string/eight_valve"
                    android:textSize="35sp"
                    android:textColor="@color/blue_dark"
                    />
                <View
                    android:id="@+id/divider_hor"
                    android:layout_width="2300dp"
                    android:layout_height="1dp"
                    android:layout_below="@id/text_eight_valve"
                    android:background="@color/blue"/>
                <View
                    android:id="@+id/divider_ver1"
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_toRightOf="@id/text_eight_valve"
                    android:background="@color/blue"/>

                <TextView
                    android:id="@+id/text_straight_way_pump"
                    android:layout_width="250dp"
                    android:layout_height="80dp"
                    android:gravity="center"
                    android:text="@string/straight_way_pump"
                    android:textSize="35sp"
                    android:textColor="@color/blue_dark"
                    android:layout_toRightOf="@id/divider_ver1"
                    />
                <View
                    android:id="@+id/divider_ver2"
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_toRightOf="@id/text_straight_way_pump"
                    android:background="@color/blue"/>

                <TextView
                    android:id="@+id/text_precise_pump"
                    android:layout_width="800dp"
                    android:layout_height="80dp"
                    android:gravity="center"
                    android:text="@string/precise_pump"
                    android:textSize="35sp"
                    android:textColor="@color/blue_dark"
                    android:layout_toRightOf="@id/divider_ver2"
                    />
                <TextView
                    android:id="@+id/text_pump_test"
                    android:layout_width="300dp"
                    android:layout_height="80dp"
                    android:gravity="center"
                    android:text="@string/pump_test"
                    android:textSize="35sp"
                    android:textColor="@color/blue_dark"
                    android:layout_below="@id/divider_hor"
                    android:layout_toRightOf="@id/divider_ver2"
                    />
                <View
                    android:id="@+id/divider_ver3"
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_toRightOf="@id/text_pump_test"
                    android:layout_below="@id/text_precise_pump"
                    android:background="@color/blue"/>
                <View
                    android:id="@+id/divider_hor2"
                    android:layout_width="800dp"
                    android:layout_height="1dp"
                    android:layout_toRightOf="@id/divider_ver2"
                    android:layout_below="@id/text_pump_test"
                    android:background="@color/blue"/>
                <TextView
                    android:id="@+id/text_pump_cal"
                    android:layout_width="500dp"
                    android:layout_height="80dp"
                    android:gravity="center"
                    android:text="@string/pump_cal"
                    android:textSize="35sp"
                    android:textColor="@color/blue_dark"
                    android:layout_below="@id/divider_hor"
                    android:layout_toRightOf="@id/divider_ver3"
                    />
                <View
                    android:id="@+id/divider_ver4"
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_toRightOf="@id/text_precise_pump"
                    android:background="@color/blue"/>
                <TextView
                    android:id="@+id/text_light_intensity"
                    android:layout_width="300dp"
                    android:layout_height="80dp"
                    android:gravity="center"
                    android:text="@string/light_intensity_test"
                    android:textSize="35sp"
                    android:textColor="@color/blue_dark"
                    android:layout_toRightOf="@id/divider_ver4"
                    />
                <View
                    android:id="@+id/divider_ver5"
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_toRightOf="@id/text_light_intensity"
                    android:background="@color/blue"/>

                <TextView
                    android:id="@+id/text_mode_temperature"
                    android:layout_width="300dp"
                    android:layout_height="80dp"
                    android:gravity="center"
                    android:text="@string/mode_temprature"
                    android:textSize="35sp"
                    android:textColor="@color/blue_dark"
                    android:layout_toRightOf="@id/divider_ver5"
                    />
                <View
                    android:id="@+id/divider_ver6"
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_toRightOf="@id/text_mode_temperature"
                    android:background="@color/blue"/>
                <TextView
                    android:id="@+id/text_lique_level"
                    android:layout_width="300dp"
                    android:layout_height="80dp"
                    android:gravity="center"
                    android:text="@string/lique_level_detect"
                    android:textSize="35sp"
                    android:textColor="@color/blue_dark"
                    android:layout_toRightOf="@id/divider_ver6"
                    />

                <!--here eight valves begin-->
                <RadioGroup
                    android:id="@+id/eight_radio1"
                    android:layout_width="140dp"
                    android:layout_height="match_parent"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:layout_below="@id/divider_hor"
                    >

                    <grean.custom.component.ToggleableRadioButton
                        android:id="@+id/valve_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="60dp"
                        android:button="@null"
                        android:drawableLeft="@android:drawable/btn_radio"
                        android:drawablePadding="10dp"
                        android:buttonTint="@color/blue_dark"
                        android:text="2"
                        android:textSize="25sp"
                        android:textColor="@color/blue_dark"
                        />
                    <grean.custom.component.ToggleableRadioButton
                        android:id="@+id/valve_4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="60dp"
                        android:button="@null"
                        android:drawableLeft="@android:drawable/btn_radio"
                        android:drawablePadding="10dp"
                        android:text="4"
                        android:textSize="25sp"
                        android:textColor="@color/blue_dark"
                        />
                    <grean.custom.component.ToggleableRadioButton
                        android:id="@+id/valve_6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="60dp"
                        android:button="@null"
                        android:drawableLeft="@android:drawable/btn_radio"
                        android:drawablePadding="10dp"
                        android:text="6"
                        android:textSize="25sp"
                        android:textColor="@color/blue_dark"
                        />
                    <grean.custom.component.ToggleableRadioButton
                        android:id="@+id/valve_8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="60dp"
                        android:button="@null"
                        android:drawableLeft="@android:drawable/btn_radio"
                        android:drawablePadding="10dp"
                        android:text="8"
                        android:textSize="25sp"
                        android:textColor="@color/blue_dark"
                        />
                </RadioGroup>



                <RadioGroup
                    android:id="@+id/eight_radio2"
                    android:layout_width="150dp"
                    android:layout_height="match_parent"
                    android:gravity="left"
                    android:orientation="vertical"
                    android:layout_toRightOf="@id/eight_radio1"
                    android:layout_below="@id/divider_hor"
                    >

                    <grean.custom.component.ToggleableRadioButton
                        android:id="@+id/valve_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="60dp"
                        android:button="@null"
                        android:drawableRight="@android:drawable/btn_radio"
                        android:drawablePadding="10dp"
                        android:buttonTint="@color/white"
                        android:text="1"
                        android:textSize="25sp"
                        android:textColor="@color/blue_dark"
                        />
                    <grean.custom.component.ToggleableRadioButton
                        android:id="@+id/valve_3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="60dp"
                        android:button="@null"
                        android:drawableRight="@android:drawable/btn_radio"
                        android:drawablePadding="10dp"
                        android:buttonTint="@color/white"
                        android:text="3"
                        android:textSize="25sp"
                        android:textColor="@color/blue_dark"
                        />
                    <grean.custom.component.ToggleableRadioButton
                        android:id="@+id/valve_5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="60dp"
                        android:button="@null"
                        android:drawableRight="@android:drawable/btn_radio"
                        android:drawablePadding="10dp"
                        android:buttonTint="@color/white"
                        android:text="5"
                        android:textSize="25sp"
                        android:textColor="@color/blue_dark"
                        />
                    <grean.custom.component.ToggleableRadioButton
                        android:id="@+id/valve_7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="60dp"
                        android:button="@null"
                        android:drawableRight="@android:drawable/btn_radio"
                        android:drawablePadding="10dp"
                        android:buttonTint="#438A7B"
                        android:text="7"
                        android:textSize="25sp"
                        android:textColor="@color/blue_dark"
                        />
                </RadioGroup>

                <ToggleButton
                    android:id="@+id/pump_9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:drawableRight="@android:drawable/btn_radio"
                    android:drawablePadding="10dp"
                    android:textOff="&#160;9&#160;"
                    android:textOn="&#160;9&#160;"
                    android:textSize="25sp"
                    android:textColor="@color/blue_dark"
                    android:layout_toRightOf="@id/divider_ver1"
                    android:layout_below="@id/divider_hor"
                    android:layout_marginLeft="85dp"
                    android:layout_marginTop="30dp"/>

                <ToggleButton
                    android:id="@+id/pump_10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:drawableRight="@android:drawable/btn_radio"
                    android:drawablePadding="10dp"
                    android:textOff="10"
                    android:textOn="10"
                    android:textSize="25sp"
                    android:textColor="@color/blue_dark"
                    android:layout_toRightOf="@id/divider_ver1"
                    android:layout_below="@id/pump_9"
                    android:layout_marginLeft="85dp"
                    android:layout_marginTop="25dp"/>

                <ToggleButton
                    android:id="@+id/pump_11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:drawableRight="@android:drawable/btn_radio"
                    android:drawablePadding="10dp"
                    android:textOff="11"
                    android:textOn="11"
                    android:textSize="25sp"
                    android:textColor="@color/blue_dark"
                    android:layout_toRightOf="@id/divider_ver1"
                    android:layout_below="@id/pump_10"
                    android:layout_marginLeft="85dp"
                    android:layout_marginTop="25dp"/>
                <ToggleButton
                    android:id="@+id/pump_12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/divider_ver1"
                    android:background="@null"
                    android:drawableRight="@android:drawable/btn_radio"
                    android:drawablePadding="10dp"
                    android:textOff="12"
                    android:textOn="12"
                    android:textSize="25sp"
                    android:textColor="@color/blue_dark"
                    android:layout_below="@id/pump_11"
                    android:layout_marginLeft="85dp"
                    android:layout_marginTop="25dp"/>
                <ToggleButton
                    android:id="@+id/pump_13"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:drawableRight="@android:drawable/btn_radio"
                    android:drawablePadding="10dp"
                    android:textOff="13"
                    android:textOn="13"
                    android:textSize="25sp"
                    android:textColor="@color/blue_dark"
                    android:layout_toRightOf="@id/divider_ver1"
                    android:layout_below="@id/pump_12"
                    android:layout_marginLeft="85dp"
                    android:layout_marginTop="25dp"/>

                <ToggleButton
                    android:id="@+id/pump_14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:button="@null"
                    android:background="@null"
                    android:drawableRight="@android:drawable/btn_radio"
                    android:textOff="14"
                    android:textOn="14"
                    android:drawablePadding="10dp"
                    android:textSize="25sp"
                    android:textColor="@color/blue_dark"
                    android:layout_toRightOf="@id/divider_ver1"
                    android:layout_below="@id/pump_13"
                    android:layout_marginLeft="85dp"
                    android:layout_marginTop="25dp"/>


                <!--pump test-->
                <TextView
                    android:layout_width="300dp"
                    android:layout_height="80dp"
                    android:layout_below="@id/divider_hor2"
                    android:layout_toRightOf="@id/divider_ver2"
                    android:text="@string/flow"
                    android:textSize="30sp"
                    android:textColor="@color/blue_dark"
                    android:gravity="center"
                    />
                <EditText
                    android:id="@+id/edit_pump_flow"
                    android:layout_width="200dp"
                    android:layout_height="60dp"
                    android:layout_below="@id/divider_hor2"
                    android:layout_toRightOf="@id/divider_ver2"
                    android:layout_marginTop="70dp"
                    android:layout_marginLeft="50dp"
                    android:imeOptions="actionDone"
                    android:inputType="numberDecimal"
                    android:maxLength="3"
                    android:gravity="center|bottom"
                    android:background="@color/gray_light"
                    android:textSize="30sp"
                    android:textColor="@color/blue_dark"/>


                <TextView
                    android:layout_width="300dp"
                    android:layout_height="80dp"
                    android:layout_below="@id/divider_hor2"
                    android:layout_toRightOf="@id/divider_ver2"
                    android:text="@string/time"
                    android:textSize="30sp"
                    android:textColor="@color/blue_dark"
                    android:gravity="center"
                    android:layout_marginTop="120dp"
                    />
                <EditText
                    android:id="@+id/edit_pump_test_time"
                    android:layout_width="200dp"
                    android:layout_height="60dp"
                    android:layout_below="@id/divider_hor2"
                    android:layout_toRightOf="@id/divider_ver2"
                    android:layout_marginTop="190dp"
                    android:layout_marginLeft="50dp"
                    android:gravity="center|bottom"
                    android:background="@color/gray_light"
                    android:imeOptions="actionDone"
                    android:inputType="numberDecimal"
                    android:maxLength="3"
                    android:textSize="30sp"
                    android:textColor="@color/blue_dark"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="60dp"
                    android:layout_below="@id/divider_hor2"
                    android:layout_toRightOf="@id/divider_ver2"
                    android:text="@string/after_ajust"
                    android:textSize="25sp"
                    android:textColor="@color/blue_dark"
                    android:gravity="center_vertical"
                    android:layout_marginTop="280dp"
                    android:layout_marginLeft="20dp"/>
                <Switch
                    android:id="@+id/pump_cal_adjust"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:checked="false"
                    android:switchTextAppearance="@drawable/switch_text_selector"
                    android:textColor="@drawable/switch_text_selector"
                    android:track="@drawable/track_drawable"
                    android:thumb="@drawable/thumb_selector"
                    android:layout_toRightOf="@id/divider_ver2"
                    android:layout_below="@id/divider_hor2"
                    android:layout_marginTop="280dp"
                    android:layout_marginLeft="150dp"/>

                <!--pump calibrate-->
                <TextView
                    android:layout_width="280dp"
                    android:layout_height="80dp"
                    android:layout_below="@id/divider_hor2"
                    android:layout_toRightOf="@id/divider_ver3"
                    android:text="@string/circles"
                    android:textSize="30sp"
                    android:textColor="@color/blue_dark"
                    android:gravity="center"
                    />
                <EditText
                    android:id="@+id/edit_circles"
                    android:layout_width="180dp"
                    android:layout_height="60dp"
                    android:layout_below="@id/divider_hor2"
                    android:layout_toRightOf="@id/divider_ver3"
                    android:layout_marginTop="70dp"
                    android:layout_marginLeft="50dp"
                    android:imeOptions="actionDone"
                    android:inputType="numberDecimal"
                    android:maxLength="3"
                    android:gravity="center|bottom"
                    android:background="@color/gray_light"
                    android:textSize="30sp"
                    android:textColor="@color/blue_dark"/>


                <TextView
                    android:layout_width="280dp"
                    android:layout_height="80dp"
                    android:layout_below="@id/divider_hor2"
                    android:layout_toRightOf="@id/divider_ver3"
                    android:text="@string/time"
                    android:textSize="30sp"
                    android:textColor="@color/blue_dark"
                    android:gravity="center"
                    android:layout_marginTop="120dp"
                    />
                <EditText
                    android:id="@+id/edit_pump_cal_time"
                    android:layout_width="180dp"
                    android:layout_height="60dp"
                    android:layout_below="@id/divider_hor2"
                    android:layout_toRightOf="@id/divider_ver3"
                    android:layout_marginTop="190dp"
                    android:layout_marginLeft="50dp"
                    android:gravity="center|bottom"
                    android:background="@color/gray_light"
                    android:imeOptions="actionDone"
                    android:inputType="numberDecimal"
                    android:maxLength="3"
                    android:textSize="30sp"
                    android:textColor="@color/blue_dark"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="60dp"
                    android:layout_below="@id/divider_hor2"
                    android:layout_toRightOf="@id/divider_ver3"
                    android:text="@string/no_adjust"
                    android:textSize="25sp"
                    android:textColor="@color/blue_dark"
                    android:gravity="center_vertical"
                    android:layout_marginTop="280dp"
                    android:layout_marginLeft="20dp"/>
                <Switch
                    android:id="@+id/pump_cal_no_adjust"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:checked="false"
                    android:switchTextAppearance="@drawable/switch_text_selector"
                    android:textColor="@drawable/switch_text_selector"
                    android:track="@drawable/track_drawable"
                    android:thumb="@drawable/thumb_selector"
                    android:layout_toRightOf="@id/divider_ver3"
                    android:layout_below="@id/divider_hor2"
                    android:layout_marginTop="280dp"
                    android:layout_marginLeft="150dp"/>


                <TextView
                    android:layout_width="280dp"
                    android:layout_height="80dp"
                    android:layout_below="@id/divider_hor2"
                    android:layout_toRightOf="@id/edit_circles"
                    android:text="@string/actual_flow"
                    android:textSize="30sp"
                    android:textColor="@color/blue_dark"
                    android:gravity="center"
                    android:layout_marginTop="120dp"
                    />

                <EditText
                    android:id="@+id/edit_pump_cal_actual_flow"
                    android:layout_width="180dp"
                    android:layout_height="60dp"
                    android:layout_below="@id/divider_hor2"
                    android:layout_toRightOf="@id/edit_circles"
                    android:layout_marginTop="190dp"
                    android:layout_marginLeft="50dp"
                    android:gravity="center|bottom"
                    android:background="@color/gray_light"
                    android:imeOptions="actionDone"
                    android:inputType="numberDecimal"
                    android:maxLength="3"
                    android:textSize="30sp"
                    android:textColor="@color/blue_dark"/>

                <Button
                    android:id="@+id/pump_cal_btn"
                    android:layout_width="180dp"
                    android:layout_height="60dp"
                    android:background="@drawable/bt_selector"
                    android:text="@string/calibration"
                    android:textColor="@drawable/text_selector"
                    android:textSize="25sp"
                    android:layout_below="@id/edit_pump_cal_actual_flow"
                    android:layout_toRightOf="@id/edit_circles"
                    android:layout_marginTop="30dp"
                    android:layout_marginLeft="50dp"
                    />

                <!--light intensity test-->
                <Button
                    android:id="@+id/btn_light_intensity"
                    android:layout_width="180dp"
                    android:layout_height="60dp"
                    android:background="@drawable/bt_selector"
                    android:text="@string/light_intensity_test"
                    android:textColor="@drawable/text_selector"
                    android:textSize="25sp"
                    android:layout_below="@id/divider_hor"
                    android:layout_toRightOf="@id/divider_ver4"
                    android:layout_marginTop="30dp"
                    android:layout_marginLeft="50dp"
                    />

                <TextView
                    android:layout_width="280dp"
                    android:layout_height="80dp"
                    android:layout_below="@id/divider_hor"
                    android:layout_toRightOf="@id/divider_ver4"
                    android:text="@string/test_light_intensity"
                    android:textSize="30sp"
                    android:textColor="@color/blue_dark"
                    android:gravity="center"
                    android:layout_marginTop="100dp"
                    />

                <EditText
                    android:id="@+id/edit_test_light_intensity"
                    android:layout_width="180dp"
                    android:layout_height="60dp"
                    android:layout_below="@id/divider_hor"
                    android:layout_toRightOf="@id/divider_ver4"
                    android:layout_marginTop="170dp"
                    android:layout_marginLeft="50dp"
                    android:gravity="center|bottom"
                    android:background="@color/gray_light"
                    android:imeOptions="actionDone"
                    android:inputType="numberDecimal"
                    android:maxLength="3"
                    android:textSize="30sp"
                    android:textColor="@color/blue_dark"/>

                <TextView
                    android:layout_width="280dp"
                    android:layout_height="80dp"
                    android:layout_below="@id/divider_hor"
                    android:layout_toRightOf="@id/divider_ver4"
                    android:layout_marginTop="230dp"
                    android:text="@string/compared_light_intensity"
                    android:textSize="30sp"
                    android:textColor="@color/blue_dark"
                    android:gravity="center"
                    />

                <EditText
                    android:id="@+id/edit_compared_light"
                    android:layout_width="180dp"
                    android:layout_height="60dp"
                    android:layout_below="@id/divider_hor"
                    android:layout_toRightOf="@id/divider_ver4"
                    android:layout_marginTop="300dp"
                    android:layout_marginLeft="50dp"
                    android:gravity="center|bottom"
                    android:background="@color/gray_light"
                    android:imeOptions="actionDone"
                    android:inputType="numberDecimal"
                    android:maxLength="3"
                    android:textSize="30sp"
                    android:textColor="@color/blue_dark"/>

    <!--temperature-->
                <Button
                    android:id="@+id/btn_tem_test"
                    android:layout_width="180dp"
                    android:layout_height="60dp"
                    android:background="@drawable/bt_selector"
                    android:text="@string/temperature_test"
                    android:textColor="@drawable/text_selector"
                    android:textSize="25sp"
                    android:layout_below="@id/divider_hor"
                    android:layout_toRightOf="@id/divider_ver5"
                    android:layout_marginTop="30dp"
                    android:layout_marginLeft="50dp"
                    />

                <TextView
                    android:layout_width="280dp"
                    android:layout_height="80dp"
                    android:layout_below="@id/divider_hor"
                    android:layout_toRightOf="@id/divider_ver5"
                    android:text="@string/test_temperature"
                    android:textSize="30sp"
                    android:textColor="@color/blue_dark"
                    android:gravity="center"
                    android:layout_marginTop="100dp"
                    />

                <EditText
                    android:id="@+id/edit_get_tem"
                    android:layout_width="180dp"
                    android:layout_height="60dp"
                    android:layout_below="@id/divider_hor"
                    android:layout_toRightOf="@id/divider_ver5"
                    android:layout_marginTop="170dp"
                    android:layout_marginLeft="50dp"
                    android:gravity="center|bottom"
                    android:background="@color/gray_light"
                    android:imeOptions="actionDone"
                    android:inputType="numberDecimal"
                    android:maxLength="3"
                    android:textSize="30sp"
                    android:textColor="@color/blue_dark"/>

                <TextView
